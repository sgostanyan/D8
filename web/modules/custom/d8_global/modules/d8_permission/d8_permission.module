<?php

/**
 * @file
 * Contains d8_permission.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function d8_permission_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the d8_permission module.
    case 'help.page.d8_permission':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Drupal 8 permissions') . '</p>';
      return $output;

    default:
  }
}
